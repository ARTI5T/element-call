{
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2016",
    "noImplicitAny": false,
    "sourceMap": false,
    "declaration": true,
    "noEmit": true,
    "jsx": "react",
    "lib": ["es2019", "dom", "dom.iterable"]
  }
}
